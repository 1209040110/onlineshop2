<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.yichen.entity">
    <class name="ShopCart" table="ShopCart" >
		<composite-id name="shopCpk" class="com.yichen.entity.ShopCartCompositePK"> 
           <key-property name="u_id" column="u_id" type="string"></key-property>
           <key-property name="p_id" column="p_id" type="string"></key-property>  
        </composite-id> 
		<property name="amount"  />
		<property name="shoptime"  />
		<property name="price"  />
		
		
    </class>
</hibernate-mapping> 